{
  "core": [
    {
      "node": {
        "type":{
          "html":"html5",
          "javascript":{
            "ecma":"2015"
          },
          "libraries":{
            "required":["jquery","generator"],
            "devRequired":["bower","booster"],
            "compileRequired":["npm","grunt"]
          }
        },
        "format":"strict",
        "prefix":"gsl",
        "closure":{
          "execute":true,
          "tags":["gsl-script", "gsl-style", "gsl-include", "gsl-meta"],
          "runOrder":1
        },
        "schema":[
          {
            "element":{
              "tag":"gsl",
              "translate":true,
              "reference":"html",
              "template":"core.page",
              "invoke":["args","params","deltas"],
              "accepts":{
                "inline":["lang","class","data","aria"]
              },
              "variants":{
                "inherits":"all",
                "tags":[
                  {
                    "markup":"if",
                    "role":"query",
                    "requires":"condition",
                    "accepts":["has","hasnot","is","isnot","like","matches"].
                    "expects":"string"
                  },
                  {
                    "markup":"elsif",
                    "role":"query",
                    "requires":"condition",
                    "accepts":["has","hasnot","is","isnot","like","matches"],
                    "expects":"string"
                  },
                  {
                    "markup":"else",
                    "role":"query",
                    "requires":null,
                    "accepts":null,
                    "expects":null
                  },
                  {
                    "markup":"data",
                    "role":"data",
                    "requires":"content",
                    "accepts":["type","reference"],
                    "expects":"string"
                  },
                  {
                    "markup":"comment",
                    "role":"literal",
                    "requires":"content",
                    "accepts":null,
                    "expects":null
                  },{
                    "markup":"note",
                    "role":"literal",
                    "requires":"content",
                    "accepts":["author","date"],
                    "expects":"string"
                  },
                  {
                    "markup":"query",
                    "role":"query",
                    "requires":"sql",
                    "accepts":null,
                    "expects":"string"
                  },
                  {
                    "markup":"markup",
                    "role":"layout",
                    "requires":"element",
                    "accepts":["repeat","content","href","class","id","data"],
                    "expects":"string"
                  }
                ]
              }
            }
          },{
            "element":{
              "tag":"head",
              "translate":false,
              "reference":"head",
              "template":"core.page",
              "invoke":["args"],
              "accepts":{
                "inline":false
              }
            }
          },{
            "element":{
              "tag":"body",
              "translate":false,
              "reference":"body",
              "template":"core.page",
              "invoke":["args","params","deltas","load"],
              "accepts":{
                "inline":["class","style","data"]
              }
            }
          }
        ],
        "compile":{
          "rules":{
            "format":{
              "lingo":"html",
              "version":5
            },
            "validations":{
              "accept":{

              },
              "reject":{

              }
            }
          }
        }
      }
    }
  ]
}